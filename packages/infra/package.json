  "scripts": {
    "build": "yarn clean && yarn ets:tsc --build && yarn build2:mjs && yarn build2:cjs && yarn generate-exports && sh ../../utils/rename.sh",
    "build2:cjs": "yarn ets:babel _esm --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .js --source-maps",
    "build2:mjs": "yarn ets:babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
    "clean": "rm -rf ./_esm && rm -rf ./_mjs && rm -rf ./_cjs && find . | egrep '\\.m?js(\\.map)?$|\\.ts(\\.map)?$|\\.tsbuildinfo' | grep -v /_src/ | grep -v .eslint | grep -v jest.config | xargs rm -f",
    "postpublish": "yarn clean",
    "generate-exports": "sh ../../utils/extract.sh > package.log",
    "prepare": "yarn build",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
    "compile": "yarn ets:tsc --noEmit",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "yarn compile && yarn lint && yarn test && yarn circular",
    "ncu": "ncu"
  },
