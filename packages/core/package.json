  "scripts": {
    "build": "yarn clean && yarn ets:tsc --build && yarn build2:mjs && yarn build2:cjs && yarn generate-exports && sh ../../utils/rename.sh && cp -f ./_src/Prelude.d.ts ./Prelude.d.ts && mv -f ./index.js ./Prelude.js && mv -f ./_mjs/index.mjs ./_mjs/Prelude.mjs",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "build2:mjs": "yarn ets:babel _esm --config-file ../../.babel.mjs.json --out-dir _mjs --out-file-extension .mjs --source-maps",
    "clean": "rm -rf ./_esm && rm -rf ./_mjs && rm -rf ./_cjs && find . | egrep '\\.m?js(\\.map)?$|\\.ts(\\.map)?$|\\.tsbuildinfo' | grep -v /types/ | grep -v /_src/ | grep -v .eslint | grep -v jest.config | xargs rm -f",
    "generate-exports": "sh ../../utils/extract.sh > package.log",
    "postpublish": "yarn clean",
    "prepare": "yarn build",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./",
    "compile": "yarn ets:tsc --noEmit",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "yarn compile && yarn lint && yarn test && yarn circular",
    "ncu": "ncu"
  },
  "publishConfig": {
    "access": "public"
  },
