{
  "name": "@effect-ts-app/boilerplate-prelude",
  "version": "0.6.1",
  "license": "MIT",
  "type": "module",
  "resolutions": {
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.156/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect-ts-app/core": "1.1.0",
    "@effect-ts-app/schema": "1.1.0",
    "@effect/core": "^0.0.16",
    "@tsplus/stdlib": "^0.2.8",
    "@effect-ts/core": "0.60.4",
    "@effect-ts/monocle": "0.43.0",
    "ts-pattern": "^4.0.6",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  },
  "peerDependencies": {
    "date-fns": "2.23.0",
    "lodash": "^4.17.21"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      },
      "require": {
        "types": "./dist/client.d.ts",
        "default": "./_cjs/client.cjs"
      }
    },
    "./schema": {
      "import": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/schema.js"
      },
      "require": {
        "types": "./dist/schema.d.ts",
        "default": "./_cjs/schema.cjs"
      }
    },
    "./_ext/lazyArg": {
      "require": {
        "types": "./dist/_ext/lazyArg.d.ts",
        "default": "./_cjs/_ext/lazyArg.cjs"
      },
      "import": {
        "types": "./dist/_ext/lazyArg.d.ts",
        "default": "./dist/_ext/lazyArg.js"
      }
    },
    "./_ext/index": {
      "require": {
        "types": "./dist/_ext/index.d.ts",
        "default": "./_cjs/_ext/index.cjs"
      },
      "import": {
        "types": "./dist/_ext/index.d.ts",
        "default": "./dist/_ext/index.js"
      }
    },
    "./_ext/Prelude.ext": {
      "require": {
        "types": "./dist/_ext/Prelude.ext.d.ts",
        "default": "./_cjs/_ext/Prelude.ext.cjs"
      },
      "import": {
        "types": "./dist/_ext/Prelude.ext.d.ts",
        "default": "./dist/_ext/Prelude.ext.js"
      }
    },
    "./_ext/EffectProgram": {
      "import": {
        "types": "./dist/_ext/EffectProgram.d.ts",
        "default": "./dist/_ext/EffectProgram.js"
      },
      "require": {
        "types": "./dist/_ext/EffectProgram.d.ts",
        "default": "./_cjs/_ext/EffectProgram.cjs"
      }
    },
    "./_ext/Pure": {
      "import": {
        "types": "./dist/_ext/Pure.d.ts",
        "default": "./dist/_ext/Pure.js"
      },
      "require": {
        "types": "./dist/_ext/Pure.d.ts",
        "default": "./_cjs/_ext/Pure.cjs"
      }
    },
    "./prelude": {
      "import": {
        "types": "./dist/prelude.d.ts",
        "default": "./dist/prelude.js"
      },
      "require": {
        "types": "./dist/prelude.d.ts",
        "default": "./_cjs/prelude.cjs"
      }
    },
    "./service": {
      "import": {
        "types": "./dist/service.d.ts",
        "default": "./dist/service.js"
      },
      "require": {
        "types": "./dist/service.d.ts",
        "default": "./_cjs/service.cjs"
      }
    },
    "./*.js": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/validator": "^13.7.10",
    "@types/faker": "5.5.9",
    "@types/uuid": "^8.3.4",
    "faker": "^5.5.3",
    "fast-check": "^3.3.0"
  },
  "scripts": {
    "build": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc --build && yarn build2:cjs",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "watch": "yarn build --watch",
    "watch2": "yarn clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc -w",
    "prepare": "yarn build",
    "clean": "rm -rf dist && rm -rf _cjs",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "circular": "yarn circular:src && yarn circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint ./_src",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "prettier": "prettier \"**/*.ts\"",
    "autofix": "yarn lint --fix",
    "test": "vitest",
    "test:run": "yarn test run --passWithNoTests",
    "testsuite": "yarn lint && yarn circular && yarn test:run",
    "ncu": "ncu -x @types/faker,faker,date-fns",
    "pub": "yarn build && npm publish --access public"
  },
  "gitHead": "bd8e27eea3eff97db8739d577d67e7336c078d28",
  "sideEffects": [
    "./dist/faker.js",
    "./faker",
    "./_src/faker",
    "./dist/faker",
    "./dist/src/faker.js",
    "./dist/faker.js"
  ]
}