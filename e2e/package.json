{
  "name": "@macs-configurator/e2e",
  "version": "1.0.0",
  "license": "NONE",
  "private": true,
  "type": "module",
  "scripts": {
    "pretest": "pnpm build",
    "test": "playwright test -c test-out",
    "test:watch": "playwright-watch test",
    "build": "tsc --build",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint .",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "up": "pnpm run update && pnpm exec-update",
    "update": "pnpm ncu -u",
    "exec-update": "pnpm i",
    "clean": "rm -rf test-out",
    "ncu": "ncu",
    "watch": "pnpm build --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@playwright/test": "~1.31.2",
    "@types/node": "~18.14.6",
    "babel-loader": "^9.1.2",
    "date-fns": "^2.29.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-check-updates": "^16.7.10",
    "playwright-core": "^1.31.2",
    "playwright-watch": "^1.3.23",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@effect-app-boilerplate/resources": "*",
    "@effect-app-boilerplate/models": "*",
    "@effect-app/prelude": "^0.25.0",
    "@effect-app/core": "^0.32.1",
    "@effect-app/schema": "^0.23.1",
    "@effect-app/vue": "^0.23.3",
    "@effect/io": "^0.6.0",
    "@effect/data": "^0.3.2",
    "@tsplus/stdlib": "^0.2.8",
    "cross-fetch": "^3.1.5",
    "tsconfig-paths": "^4.1.2"
  }
}