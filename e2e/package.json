    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@playwright/test": "~1.34.3",
    "@types/node": "~20.2.5",
    "babel-loader": "^9.1.2",
    "date-fns": "^2.30.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
