{
    "name": "@effect-ts-app/boilerplate-root",
    "private": true,
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "clean": "ultra -r clean",
        "clean-dist": "ultra -r clean-dist",
        "upgrade": "echo hi",
        "client": "cd packages/client && pnpm client",
        "scan": "pnpm esw -w --fix apps/api-api/_src/Usecases.ts packages/client/_src/Resources.ts packages/client/_src/Resources/*.ts",
        "autofix": "NODE_OPTIONS=--max-old-space-size=6144 ultra -r autofix",
        "lint": "ultra -r lint",
        "circular:dist": "ultra -r circular:dist",
        "start": "concurrently -k -p \"[{name}]\" -n \"API,Frontend\" -c \"cyan.bold,green.bold,blue.bold\" \"pnpm start:api\" \"pnpm start:frontend\"",
        "start:api": "cd apps/api && pnpm start",
        "start:frontend": "cd apps/frontend && pnpm start",
        "dev": "pnpm build:e2e && concurrently -k -p \"[{name}]\" -n \"Watch,API,Frontend\" -c \"yellow.bold,cyan.bold,green.bold\" \"pnpm watch\" \"pnpm dev:api\" \"pnpm dev:frontend\"",
        "online-dev": "cd apps/frontend && pnpm online-dev",
        "dev:api": "cd apps/api && pnpm dev",
        "dev:frontend": "cd apps/frontend && pnpm dev",
        "build-apps": "cd apps/api && pnpm build && cd ../frontend && pnpm build",
        "postinstall": "patch-package",
        "test": "ultra -r pnpm test",
        "testsuite": "ultra -r pnpm testsuite",
        "up": "pnpm run update && pnpm run exec-update",
        "update": "ultra -r --root ncu -u --packageFile package.json",
        "exec-update": "pnpm update",
        "ncu": "ncu",
        "build-libs": "cd packages/prelude && pnpm build && cd ../types && pnpm build && cd ../client && pnpm build && cd ../vue && pnpm build && cd ../infra && pnpm build && cd ../messages && pnpm build && cd ../../libs/eslint && pnpm build",
        "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx",
        "ultra-api": "ultra --concurrency 1 --filter \"apps/api*\" -r",
        "ultra-libs": "ultra --concurrency 1 --filter \"packages/*\" -r",
        "circular-api": "pnpm ultra-api circular",
        "circular-libs": "pnpm ultra-libs circular",
        "lint-api": "pnpm ultra-api lint",
        "lint-libs": "pnpm ultra-libs lint",
        "test-api": "pnpm ultra-api test:run",
        "test-libs": "pnpm ultra-libs test:run",
        "testsuite-api": "pnpm ultra-api testsuite",
        "testsuite-libs": "pnpm ultra-libs testsuite",
        "testsuite-libs-manual": "cd packages/prelude && pnpm testsuite && cd ../types && pnpm testsuite && cd ../client && pnpm testsuite && cd ../infra && pnpm testsuite && cd ../messages && pnpm testsuite && cd ../vue && pnpm testsuite",
        "build-api": "cd apps/api && pnpm build",
        "watch-libs": "pnpm build-libs && ultra -r --filter \"packages/*\" watch",
        "build": "NODE_OPTIONS=--max-old-space-size=6144 tsc --build ./tsconfig.all.json",
        "build-prod": "pnpm build-libs && pnpm build-frontend-prod",
        "build-frontend-prod": "cd apps/frontend && pnpm build",
        "build-e2e": "cd apps/api && pnpm e2e:build",
        "watch": "pnpm build-api || echo build-fail && concurrently -k -p \"[{name}]\" -n \"API,Frontend\" -c \"cyan.bold,green.bold\" \"cd apps/api && pnpm build -w\" \"cd apps/frontend && pnpm compile -w\""
    },
    "resolutions": {
        "@effect/core": "^0.0.15",
        "@types/node": "~18.11.9",
        "@types/react": "^18.0.25",
        "http-proxy": "npm:http-proxy-node16@1.0.0",
        "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.156/compiler/typescript.tgz"
    },
    "dependencies": {
        "@effect-ts-app/boilerplate-prelude": "workspace:*",
        "@effect-ts-app/boilerplate-eslint": "workspace:*",
        "@effect-ts-app/core": "1.1.4",
        "@effect-ts-app/schema": "1.1.4",
        "@effect-ts/core": "^0.60.4",
        "@effect-ts/system": "^0.57.4",
        "@swc/helpers": "0.4.14",
        "@types/react": "^18.0.25",
        "cross-env": "^7.0.3",
        "http-proxy": "npm:http-proxy-node16@1.0.0",
        "@effect-ts/monocle": "^0.43.0",
        "patch-package": "^6.5.0",
        "postinstall-postinstall": "^2.1.0",
        "ts-lib": "^0.0.5",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@phaphoso/eslint-plugin-dprint": "^0.5.2",
        "@swc/core": "1.3.20",
        "@swc/register": "^0.1.10",
        "@tsconfig/strictest": "^1.0.2",
        "@tsplus/installer": "0.0.159",
        "@types/lodash": "^4.14.190",
        "@types/node": "~18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "@typescript-eslint/scope-manager": "^5.44.0",
        "babel-plugin-replace-import-extension": "^1.1.3",
        "concurrently": "^7.6.0",
        "dprint": "^0.33.0",
        "enhanced-resolve": "^5.12.0",
        "eslint": "^8.28.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-codegen": "^0.16.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-sort-destructure-keys": "^1.4.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-watch": "^8.0.0",
        "fast-check": "^3.3.0",
        "history": "^5.3.0",
        "json5": "^2.2.1",
        "lerna": "^6.0.3",
        "madge": "^5.0.1",
        "module-alias": "^2.2.2",
        "nodemon": "^2.0.20",
        "npm-check-updates": "^16.4.3",
        "ts-transform-paths": "^2.0.3",
        "tsc-watch": "^5.0.3",
        "tsconfig-paths": "^4.1.0",
        "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.156/compiler/typescript.tgz",
        "typescript-transform-paths": "^3.4.4",
        "ultra-runner": "^3.10.5",
        "vitest": "^0.25.3"
    }
}